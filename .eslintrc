{
  "rules": {
    "curly": [
      2
    ],
    "eqeqeq": [
      2
    ],
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "new-cap": [
      2,
      {
        "properties": false
      }
    ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-use-before-define": [
      2
    ],
    "no-trailing-spaces": [
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ],
    "wrap-iife": [
      2,
      "outside"
    ],
  },
  "globals": {
    "after": true,
    "afterEach": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
    "it": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}